<h1>What Have You Done?</h1>
      <h3>{{zip_code}}</h3>

<div class='body'>
  <div class="house">
    <h1>House</h1>
    {{#each localReps.house as |rep|}}
    <div class="rep">
      <div class='image'>
      <img src={{rep.photo}}
      </div>
      <section>
        <h2>{{rep.name}}</h2>
        <h3>{{rep.party}}</h3>
      </section>
    </div>
    {{/each}}
  </div>

    <div class="senate">
      <h1>Senate</h1>
      {{#each localReps.senate as |rep|}}
      <div class="rep">
        <div class='image'>
        <img src={{rep.photo}}
        </div>
        <section>
          <h2>{{rep.name}}</h2>
          <h3>{{rep.party}}</h3>
        </section>
        </div>
      {{/each}}
    </div>
  </div>
